    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:largeHeap="true"
        android:name="com.front_end.app.appcomponents.di.MyApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/Theme.MyApp" android:usesCleartextTraffic="true">
        <activity android:exported="true"
            android:name="com.front_end.app.modules.welcomescreen.ui.WelcomeScreenActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.front_end.app.modules.tall.ui.TallActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.front_end.app.modules.target.ui.TargetActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.front_end.app.modules.age.ui.AgeActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.front_end.app.modules.weight.ui.WeightActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.front_end.app.modules.signup.ui.SignUpActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.front_end.app.modules.login.ui.LoginActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.front_end.app.modules.loadingpage.ui.LoadingPageActivity" android:screenOrientation="portrait"/>
    </application>
</manifest>
